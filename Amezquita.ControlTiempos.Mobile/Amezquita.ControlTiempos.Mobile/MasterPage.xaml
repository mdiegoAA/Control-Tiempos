<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Amezquita.ControlTiempos.Mobile.MasterPage"
             Title="Amezquita Control de Tiempos"
             Padding="10, 10, 10, 10">
  <ContentPage.Content>
    <StackLayout VerticalOptions="StartAndExpand">
      <Button Text="Registrar Tiempos" Clicked="RegistrarTiempos"></Button>
      <Button Text="Historial" Clicked="Historial"></Button>
      <Button Text="Cerrar Sesion" Clicked="CerrarSesion"></Button>
    </StackLayout>
  </ContentPage.Content>
</ContentPage>