        <Tables>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="HoraAreaTableAdapter" GeneratorDataComponentClassName="HoraAreaTableAdapter" Name="HoraArea" UserDataComponentName="HoraAreaTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ControlTiemposDbContext (Web.config)" DbObjectName="SELECT CA.CargoId , CA.NombreCargo , CA.Tarifa , DATEPART(MONTH, CH.FechaInicio) Mes , DATEDIFF(MINUTE, CH.FechaInicio, CH.FechaFin) AS TotalHoras FROM CarguesHoras AS CH INNER JOIN Proyectos AS P ON CH.ProyectoId = P.ProyectoId INNER JOIN Clientes AS CL ON P.ClienteId = CL.ClienteId INNER JOIN Usuarios AS U ON CH.UsuarioId = U.UsuarioId INNER JOIN Cargos AS CA ON U.CargoId = CA.CargoId WHERE CH.EsNovedad = 0 AND CH.Aprobada = 1 AND (@ClienteId IS NULL OR P.ClienteId = @ClienteId) AND (@DirectorId IS NULL OR P.DirectorId = @DirectorId) AND CH.FechaInicio BETWEEN @FechaInicio AND @FechaFin" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
      R.CargoId
    , R.NombreCargo
    , R.Tarifa
    , SUM(CASE WHEN R.Mes =  1 THEN R.TotalHoras/60 ELSE 0 END) AS Ene
    , SUM(CASE WHEN R.Mes =  2 THEN R.TotalHoras/60 ELSE 0 END) AS Feb
    , SUM(CASE WHEN R.Mes =  3 THEN R.TotalHoras/60 ELSE 0 END) AS Mar
    , SUM(CASE WHEN R.Mes =  4 THEN R.TotalHoras/60 ELSE 0 END) AS Abr
    , SUM(CASE WHEN R.Mes =  5 THEN R.TotalHoras/60 ELSE 0 END) AS May
    , SUM(CASE WHEN R.Mes =  6 THEN R.TotalHoras/60 ELSE 0 END) AS Jun
    , SUM(CASE WHEN R.Mes =  7 THEN R.TotalHoras/60 ELSE 0 END) AS Jul
    , SUM(CASE WHEN R.Mes =  8 THEN R.TotalHoras/60 ELSE 0 END) AS Ago
    , SUM(CASE WHEN R.Mes =  9 THEN R.TotalHoras/60 ELSE 0 END) AS Sep
    , SUM(CASE WHEN R.Mes = 10 THEN R.TotalHoras/60 ELSE 0 END) AS Oct
    , SUM(CASE WHEN R.Mes = 11 THEN R.TotalHoras/60 ELSE 0 END) AS Nov
    , SUM(CASE WHEN R.Mes = 12 THEN R.TotalHoras/60 ELSE 0 END) AS Dic
    , SUM(R.TotalHoras) AS TotalHoras
    , SUM(R.TotalHoras) * R.Tarifa AS Costo
FROM (
    SELECT
      CA.CargoId
    , CA.NombreCargo
    , CA.Tarifa
    , DATEPART(MONTH, CH.FechaInicio) Mes
    , DATEDIFF(MINUTE, CH.FechaInicio, CH.FechaFin) AS TotalHoras
    FROM CarguesHoras AS CH 
    INNER JOIN Proyectos AS P ON CH.ProyectoId = P.ProyectoId
    INNER JOIN Clientes AS CL ON P.ClienteId = CL.ClienteId
    INNER JOIN Usuarios AS U ON CH.UsuarioId = U.UsuarioId
    INNER JOIN Cargos AS CA ON U.CargoId = CA.CargoId
    WHERE CH.EsNovedad = 0 AND CH.Aprobada = 1 AND (@ClienteId IS NULL OR P.ClienteId = @ClienteId) AND (@DirectorId  IS NULL OR P.DirectorId = @DirectorId) AND CH.FechaInicio BETWEEN @FechaInicio AND @FechaFin) R
GROUP BY R.CargoId, R.NombreCargo, R.Tarifa</CommandText>
